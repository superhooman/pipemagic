{
  "name": "pipemagic",
  "version": "0.1.0",
  "description": "Standalone runtime for PipeMagic image processing pipelines",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "peerDependencies": {
    "@huggingface/transformers": "^3.4.0"
  },
  "peerDependenciesMeta": {
    "@huggingface/transformers": {
      "optional": true
    }
  },
  "devDependencies": {
    "@webgpu/types": "^0.1.69",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3"
  },
  "license": "MIT"
}
